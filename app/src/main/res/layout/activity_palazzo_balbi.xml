<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="0px"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.android.ggallery.museodellemarionette.PalazzoBalbi">
    <ImageView
        android:layout_width="800px"
        android:layout_height="157dp"
        android:id="@+id/ombra2"
        android:src="@drawable/ombra"

        android:layout_marginTop="1450px"

        android:layout_marginLeft="340px" />
    <WebView
        android:layout_width="800px"
        android:layout_height="1300px"
        android:id="@+id/webView"
        android:layout_alignParentTop="true"
        android:layout_marginTop="240px"
        android:layout_alignParentEnd="false"
        android:layout_alignParentStart="false"
        android:layout_marginLeft="340px" />


    <ImageView
        android:layout_width="1000dp"
        android:layout_height="match_parent"
        android:id="@+id/sipario"
        android:src="@drawable/sipario"
        android:paddingLeft="-100px"
        android:paddingTop="-100px"
        android:layout_marginTop="-250px" />

    <ImageView
        android:layout_width="875px"
        android:layout_height="200px"
        android:id="@+id/scrittamuseo"
        android:src="@drawable/scrittamuseo"
        android:layout_marginTop="0px"
        android:paddingLeft="-100px"/>

</RelativeLayout>
